

<% include partials/navbar.ejs %>

<body>

<form class="form-horizontal" action="/home/create" method="post">
  <fieldset>

  <!-- Form Name -->
  <legend>Add an Article</legend>


  <!-- Title -->
  <div class="form-group">
    <label class="col-md-4 control-label" for="subject">Article Title</label>
    <div class="col-md-4">
    <input id="title" name="title" type="text" placeholder="put title here" class="form-control input-md">

    </div>
  </div>

  <!-- url -->
  <div class="form-group">
    <label class="col-md-4 control-label" for="content">Please enter url</label>
    <div class="col-md-4">
    <input id="url" name="url" type="text" placeholder="url placeholder" class="form-control input-md">

    </div>
  </div>

  <!-- Button -->
  <div class="form-group">
    <label class="col-md-4 control-label" for="submit"></label>
    <div class="col-md-4">
      <button id="submit" name="submit" class="btn btn-success">Submit</button>
    </div>
  </div>


  </fieldset>
</form>



<% include partials/footer.ejs %>

</body>
